{
  "name": "Track Order - Israel Mail",
  "version": "1.0",
  "description": "Build an Extension!",
  "permissions": ["activeTab", 
    "contextMenus",
    "https://mypost.israelpost.co.il/"],
  "browser_action" : {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    },
    "persistent": true
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "background": {
    "scripts": ["background.js", "track_order.js"]
  },
  "manifest_version": 2
}
